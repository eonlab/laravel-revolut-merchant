{
    "name": "eonlab/laravel-revolut-merchant",
    "description": "An unoffial Laravel wrapper for Revolut's Merchant API",
    "license": "MIT",
    "require": {
        "guzzlehttp/guzzle": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Eonlab\\RevolutMerchant\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eonlab\\RevolutMerchant\\Providers\\RevolutMerchantServiceProvider"
            ],
            "aliases": {
                "Merchant": "Eonlab\\RevolutMerchant\\Facades\\Merchant"
            }
        }
    },
    "require-dev": {
        "orchestra/testbench": "^5.1"
    }
}
